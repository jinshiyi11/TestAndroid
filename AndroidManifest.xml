<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shuai.test"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />
    
    <uses-permission android:name="android.permission.GET_TASKS"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".TestBroadcastActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="com.shuai.test.layout.TestLayoutActivity" >
        </activity>
        <activity
            android:name="com.shuai.test.touchevent.TestTouchActivity"
            android:label="@string/title_activity_test_touch" >
        </activity>
        <activity
            android:name="com.shuai.test.dialog.TestDialogActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".tools.TopActivityMonitorService"></service>
        <service
        android:label="WindowChangeDetectingService"
        android:name="com.shuai.test.tools.WindowChangeDetectingService"
        android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE">
        <intent-filter>
            <action android:name="android.accessibilityservice.AccessibilityService"/>
        </intent-filter>
        <meta-data
            android:name="android.accessibilityservice"
            android:resource="@xml/accessibilityservice"/>
    </service>
        
        
    </application>

</manifest>
